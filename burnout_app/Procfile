web: npm run build 2>&1 || echo "Build failed, continuing..." && mkdir -p public/build && php artisan config:clear && php artisan cache:clear && php artisan route:clear && php artisan view:clear && php artisan config:cache && php artisan storage:link || true && (php artisan migrate --force --seed || echo "Migration failed") && php artisan serve --host=0.0.0.0 --port=$PORT

